{
  "expo": {
    "name": "Money Tracker",
    "slug": "myexpense",
    "version": "1.0.0",
    "orientation": "portrait",
    "scheme": "myexpense",
    "userInterfaceStyle": "automatic",
    "newArchEnabled": true,
    "ios": {
      "supportsTablet": true,
      "bundleIdentifier": "com.tahajad.MyExpense",
      "icon" : {
        "dark" : "./assets/icons/adaptive-icon.png",
        "light" : "./assets/icons/adaptive-icon.png",
        "tinted" : "./assets/icons/adaptive-icon.png"
      }
    },
    "android": {
      "adaptiveIcon": {
        "foregroundImage": "./assets/icons/adaptive-icon.png",
        "backgroundColor": "#ffffff"
      },
      "package": "com.tahajad.MyExpense"
    },
    "splash": {
      "image": "./assets/images/icon.png",
      "resizeMode": "contain",
      "backgroundColor": "#ffffff"
    },
    "web": {
      "bundler": "metro",
      "output": "static",
      "favicon": "./assets/images/favicon.png"
    },
    "plugins": [
      "expo-router",
      [
        "expo-splash-screen",
        {
          "image": "./assets/images/icon.png",
          "imageWidth": 200,
          "resizeMode": "contain",
          "backgroundColor": "#ffffff"
        }
      ],
      "expo-font",
      "expo-web-browser",
      "expo-localization"
    ],
    "experiments": {
      "typedRoutes": true
    },
    "extra": {
      "router": {},
      "supportsRTL": true,
      "eas": {
        "projectId": "248b8569-7312-45ad-9886-7b4fb2660d03"
      }
    }
  }
}
